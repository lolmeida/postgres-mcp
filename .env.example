# Configurações do Banco de Dados
DB_HOST=localhost
DB_PORT=5432
DB_NAME=postgres
DB_USER=postgres
DB_PASSWORD=postgres

# Configurações do Servidor MCP
MCP_MODE=http
MCP_PORT=8432
MCP_LOG_LEVEL=info
MCP_LOG_SQL=true

# Configurações de Pool de Conexões
POOL_MIN_SIZE=1
POOL_MAX_SIZE=10

# Configurações de Timeouts
COMMAND_TIMEOUT=30
TRANSACTION_TIMEOUT=300

# Configurações de Limites
MAX_QUERY_ROWS=5000

# Configurações de Cache
CACHE_MAX_SIZE=1000
CACHE_TTL=3600
